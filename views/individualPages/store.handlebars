<section class="store">
    <h2>Store</h2>
    <h3>Treble Clef Songs</h3>
    <section class="storeBody">
        {{#each trebleSongs}}
            <section class="storeCard">
                <img src="{{picture}}" alt="{{name}} image" class="cardImage"/>
                <p class="cardName">{{name}}</p>
                <p class="cardPrice">{{price}}</p>
            </section>
        {{/each}}
    </section>


    <h3>Bass Clef Songs</h3>
    <section class="storeBody">
        {{#each bassSongs}}
            <section class="storeCard">
                <img src="{{picture}}" alt="{{name}} image" class="cardImage"/>
                <p class="cardName">{{name}}</p>
                <p class="cardPrice">{{price}}</p>
            </section>
        {{/each}}
    </section>
</section>