<section class="reportCard">
	<p class="reportGrade animateStamp">A{{grade}}</p>
	<h2>Report Card</h2>
	<h3>you {{result}}</h3>
	<table class="reportDetailsTable ">
		<tbody>
			<tr>
				<td>Score</td>
				<td>{{score}}</td>
			</tr>
			<tr>
				<td>Accuracy</td>
				<td>{{accuracy}}%</td>
			</tr>
			<tr>
				<td>Highest Streak</td>
				<td>{{streak}}</td>
			</tr>
			{{#if fail}}
				<tr>
					<td>Passing Score</td>
					<td>{{minScore}}</td>
				</tr>
				<tr>
					<td>Passing Accuracy</td>
					<td>80%</td>
				</tr>
			{{/if}}

		</tbody>
	</table>
	{{!-- <p>Score</p><p>{{score}}</p>
	<p>Highest Streak: {{streak}}</p>
	<p>Accuracy: {{accuracy}}%</p> --}}
	<section>

	<section class="cardNav">
		{{#if fail}}
			<button id='failBtn' class='actionButton' onClick="nextLesson(true)">Try Again</button>
			{{else}}
				<button id='passBtn' class='actionButton' onClick="nextLesson()">Next Level</button>
		{{/if}}
	</section>
</section>
